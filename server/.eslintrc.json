{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "off",
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^"
            }
        ]
    },
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
    ]
}